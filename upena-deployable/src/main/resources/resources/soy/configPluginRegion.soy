 * @param? readWrite
 */
{template .configPluginRegion}
    <div id="upena-cfg-pending">
        <div class="upena-cfg-pending-container">
            Uncommitted changes for <span id="pending-count">0</span> properties
            {if $readWrite}
                <button class="btn btn-default" id="pending-save" style="margin-left:20px;">Save changes</button>
            {/if}
        </div>
    </div>
    <div class="panel panel-info">
        <div class="panel-heading"><span class="panel-title">Config</span></div>
        {if $message}
            <div class="panel-heading">
                <pre>{$message}</pre>
            </div>
        {/if}
        <form id="config" class="j-form" data-active-class="btn-default" data-input-el="#requestPicker" data-form-el="#requestPickerForm"
            data-jive-ui="jive/ui/typeahead/request_typeahead" method="post" name="config">
            <table id="upena-cfg" class="table table-condensed table-responsive">
                <tbody>
                    <tr>
                        <td colspan="2" bgcolor="#8299AF">
                            <div class="input-group">
                                <div class="input-group-addon" id="basic-addon1">
                                    <img src="/static/img/cluster.png" alt="Cluster" style="width:20px;height:20px;">&nbsp
                                </div>
                                <input type="text" autocomplete="off" role="combobox" style="padding-left:10px;"
                                    placeholder="Cluster" value="{$aFilters.cluster?:''}" name="aCluster" class="form-control upena-hs-field"
                                    data-upena-lookup="/ui/lookup/clusters" data-upena-remote="a">
                                <input type="hidden" name="aClusterKey" value="{$aFilters.clusterKey}" />
                            </div>
                        </td>
                        <td style="width:100px;" bgcolor="#8299AF"></td>
                        <td style="width:100px;" bgcolor="#8299AF"></td>
                        <td style="width:100px;" bgcolor="#8299AF"></td>
                        <td bgcolor="#8299AF"></td>
                        <td bgcolor="#8299AF"></td>
                        <td bgcolor="#FFFFFF" style="width:100px; text-align: center;">Cluster</td>
                        <td bgcolor="#97979F"></td>
                        <td bgcolor="#97979F"></td>
                        <td style="width:100px;" bgcolor="#97979F"></td>
                        <td style="width:100px;" bgcolor="#97979F"></td>
                        <td style="width:100px;" bgcolor="#97979F"></td>
                        <td  colspan="2" bgcolor="#97979F">
                            <div class="input-group">
                                <div class="input-group-addon" id="basic-addon1">
                                    <img src="/static/img/cluster.png" alt="Cluster" style="width:20px;height:20px;">&nbsp
                                </div>
                                <input type="text" autocomplete="off" role="combobox" style="padding-left:10px;"
                                    placeholder="Cluster" value="{$bFilters.cluster?:''}" name="bCluster" class="form-control upena-hs-field"
                                    data-upena-lookup="/ui/lookup/clusters" data-upena-remote="b">
                                <input type="hidden" name="bClusterKey" value="{$bFilters.clusterKey}" />
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:100px;" bgcolor="#8299AF"></td>
                        <td  colspan="2" bgcolor="#92A9BF">
                            <div class="input-group">
                                <div class="input-group-addon" id="basic-addon1">
                                    <img src="/static/img/host.png" alt="Host" style="width:20px;height:20px;">&nbsp
                                </div>
                                <input type="text" autocomplete="off" role="combobox" style="padding-left:10px;"
                                    placeholder="Host" value="{$aFilters.host?:''}" name="aHost" class="form-control upena-hs-field"
                                    data-upena-lookup="/ui/lookup/hosts" data-upena-remote="a">
                                <input type="hidden" name="aHostKey" value="{$aFilters.hostKey}" />
                            </div>
                        </td>
                        <td style="width:100px;" bgcolor="#92A9BF"></td>
                        <td style="width:100px;" bgcolor="#92A9BF"></td>
                        <td bgcolor="#92A9BF"></td>
                        <td bgcolor="#92A9BF"></td>
                        <td bgcolor="#FFFFFF" style="width:100px; text-align: center;">Host</td>
                        <td bgcolor="#A7A7AF"></td>
                        <td bgcolor="#A7A7AF"></td>
                        <td style="width:100px;" bgcolor="#A7A7AF"></td>
                        <td style="width:100px;" bgcolor="#A7A7AF"></td>
                        <td  colspan="2" bgcolor="#A7A7AF">
                            <div class="input-group">
                                <div class="input-group-addon" id="basic-addon1">
                                    <img src="/static/img/host.png" alt="Host" style="width:20px;height:20px;">&nbsp
                                </div>
                                <input type="text" autocomplete="off" role="combobox" style="padding-left:10px;"
                                    placeholder="Host" value="{$bFilters.host?:''}" name="bHost" class="form-control upena-hs-field"
                                    data-upena-lookup="/ui/lookup/hosts" data-upena-remote="b">
                                <input type="hidden" name="bHostKey" value="{$bFilters.hostKey}" />
                            </div>
                        </td>
                        <td style="width:100px;" bgcolor="#97979F"></td>
                    </tr>
                    <tr>
                        <td style="width:100px;" bgcolor="#8299AF"></td>
                        <td style="width:100px;" bgcolor="#92A9BF"></td>
                        <td colspan="2" bgcolor="#A2B9CF">
                            <div class="input-group">
                                <div class="input-group-addon" id="basic-addon1">
                                    <img src="/static/img/service.png" alt="Service" style="width:20px;height:20px;">&nbsp
                                </div>
                                <input type="text" autocomplete="off" role="combobox" style="padding-left:10px;"
                                    placeholder="Service" value="{$aFilters.service?:''}" name="aService" class="form-control upena-hs-field"
                                    data-upena-lookup="/ui/lookup/services" data-upena-remote="a">
                                <input type="hidden" name="aServiceKey" value="{$aFilters.serviceKey}" />
                            </div>
                        </td>
                        <td style="width:100px;" bgcolor="#A2B9CF"></td>
                        <td bgcolor="#A2B9CF"></td>
                        <td bgcolor="#A2B9CF"></td>
                        <td bgcolor="#FFFFFF" style="width:100px; text-align: center;">Service</td>
                        <td bgcolor="#B7B7BF"></td>
                        <td bgcolor="#B7B7BF"></td>
                        <td style="width:100px;" bgcolor="#B7B7BF"></td>
                        <td colspan="2" bgcolor="#B7B7BF">
                            <div class="input-group">
                                <div class="input-group-addon" id="basic-addon1">
                                    <img src="/static/img/service.png" alt="Service" style="width:20px;height:20px;">&nbsp
                                </div>
                                <input type="text" autocomplete="off" role="combobox" style="padding-left:10px;"
                                    placeholder="Service" value="{$bFilters.service?:''}" name="bService" class="form-control upena-hs-field"
                                    data-upena-lookup="/ui/lookup/services" data-upena-remote="b">
                                <input type="hidden" name="bServiceKey" value="{$bFilters.serviceKey}" />
                            </div>
                        </td>
                        <td style="width:100px;" bgcolor="#A7A7AF"></td>
                        <td style="width:100px;" bgcolor="#97979F"></td>
                    </tr>
                    <tr>
                        <td style="width:100px;" bgcolor="#8299AF"></td>
                        <td style="width:100px;" bgcolor="#92A9BF"></td>
                        <td style="width:100px;" bgcolor="#A2B9CF"></td>
                        <td  colspan="2" bgcolor="#B2C9DF">
                            <div class="input-group">
                                <div class="input-group-addon" id="basic-addon1">
                                    <img src="/static/img/instance.png" alt="Instance" style="width:20px;height:20px;">&nbsp
                                </div>
                                <input type="text" autocomplete="off" role="combobox" style="padding-left:10px;"
                                    placeholder="Instance" value="{$aFilters.instance?:''}" name="aInstance" class="form-control">
                            </div>
                        </td>
                        <td bgcolor="#B2C9DF"></td>
                        <td bgcolor="#B2C9DF"></td>
                        <td bgcolor="#FFFFFF" style="width:100px; text-align: center;">Instance</td>
                        <td bgcolor="#C7C7CF"></td>
                        <td bgcolor="#C7C7CF"></td>
                        <td  colspan="2" bgcolor="#C7C7CF">
